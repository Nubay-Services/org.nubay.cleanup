  <license>AGPL-3.0</license>
  <maintainer>
    <author>Phil Currier</author>
    <email>pcurrier@nubay.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/Nubay-Services/org.nubay.prunelogs</url>
    <url desc="Documentation">https://github.com/Nubay-Services/org.nubay.prunelogs</url>
    <url desc="Support">https://github.com/Nubay-Services/org.nubay.prunelogs/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-02-19</releaseDate>
  <version>0.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
